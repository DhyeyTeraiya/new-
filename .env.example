# =============================================================================
# BROWSER AI AGENT PRO - ENTERPRISE CONFIGURATION
# Superior to Manus AI with Advanced Multi-Agent Architecture
# =============================================================================

# =============================================================================
# ENVIRONMENT CONFIGURATION
# =============================================================================
NODE_ENV=development
API_VERSION=v2
API_PREFIX=/api/v2

# =============================================================================
# DATABASE CONFIGURATION (MongoDB - Superior to PostgreSQL for AI workloads)
# =============================================================================
MONGODB_URI=mongodb://admin:enterprise123@localhost:27017/browser_ai_agent?authSource=admin
MONGODB_DB_NAME=browser_ai_agent

# =============================================================================
# REDIS CLUSTER CONFIGURATION (Superior performance vs single Redis)
# =============================================================================
REDIS_MASTER_URL=redis://localhost:6379
REDIS_SLAVE_URL=redis://localhost:6380
REDIS_CLUSTER_ENABLED=true

# =============================================================================
# JWT CONFIGURATION (Enterprise-grade security)
# =============================================================================
JWT_SECRET=enterprise-jwt-secret-superior-to-manus-ai-change-in-production
JWT_REFRESH_SECRET=enterprise-refresh-secret-superior-to-manus-ai-change-in-production
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# =============================================================================
# API CONFIGURATION (Fastify - Faster than Express)
# =============================================================================
PORT=4000
AGENTS_PORT=4001
CORS_ORIGIN=http://localhost:3000

# =============================================================================
# MULTI-AGENT SYSTEM CONFIGURATION (5-Agent vs Manus 3-Agent)
# =============================================================================
AGENT_PLANNER_ENABLED=true
AGENT_NAVIGATOR_ENABLED=true
AGENT_EXTRACTOR_ENABLED=true
AGENT_VERIFIER_ENABLED=true
AGENT_COORDINATOR_ENABLED=true
AGENT_MAX_CONCURRENT_TASKS=100
AGENT_TASK_TIMEOUT=300000

# =============================================================================
# AI API KEYS (Multi-LLM Integration - Superior to Manus Claude+Qwen)
# =============================================================================
NVIDIA_API_KEY=your-nvidia-nim-api-key
NVIDIA_API_BASE_URL=https://api.nvidia.com

OPENAI_API_KEY=your-openai-gpt4-api-key
OPENAI_API_BASE_URL=https://api.openai.com/v1

ANTHROPIC_API_KEY=your-anthropic-claude-api-key
ANTHROPIC_API_BASE_URL=https://api.anthropic.com

GOOGLE_AI_API_KEY=your-google-gemini-api-key

# =============================================================================
# BROWSER AUTOMATION CONFIGURATION (Advanced Playwright)
# =============================================================================
PLAYWRIGHT_BROWSERS_PATH=/ms-playwright
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=30000
PLAYWRIGHT_MAX_CONCURRENT_BROWSERS=10
PLAYWRIGHT_PROXY_ROTATION=true
PLAYWRIGHT_STEALTH_MODE=true

# =============================================================================
# SECURITY CONFIGURATION (Military-grade - Superior to Manus AES-256)
# =============================================================================
ENCRYPTION_KEY=your-aes-256-gcm-encryption-key-32-chars
ENCRYPTION_IV=your-initialization-vector-16-chars
SECURITY_HEADERS_ENABLED=true
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000
DDOS_PROTECTION_ENABLED=true

# =============================================================================
# CLOUD EXECUTION CONFIGURATION (Kubernetes-ready)
# =============================================================================
CLOUD_EXECUTION_ENABLED=true
KUBERNETES_NAMESPACE=browser-ai-agent
AUTO_SCALING_ENABLED=true
MIN_REPLICAS=2
MAX_REPLICAS=100
CPU_THRESHOLD=70
MEMORY_THRESHOLD=80

# =============================================================================
# FILE STORAGE CONFIGURATION (S3-compatible)
# =============================================================================
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET_NAME=browser-ai-agent-storage
MINIO_USE_SSL=false

# =============================================================================
# NOTIFICATION CONFIGURATION (Multi-channel)
# =============================================================================
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-app-password
EMAIL_FROM=noreply@browser-ai-agent.com

SLACK_WEBHOOK_URL=your-slack-webhook-url
DISCORD_WEBHOOK_URL=your-discord-webhook-url
SMS_API_KEY=your-sms-api-key

# =============================================================================
# MONITORING & LOGGING CONFIGURATION (Enterprise-grade)
# =============================================================================
LOG_LEVEL=info
LOG_FILE=logs/browser-ai-agent.log
LOG_CONSOLE=true
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true
SENTRY_DSN=your-sentry-dsn
DATADOG_API_KEY=your-datadog-api-key

# =============================================================================
# FRONTEND CONFIGURATION (Next.js 14 with Advanced Features)
# =============================================================================
NEXT_PUBLIC_API_URL=http://localhost:4000
NEXT_PUBLIC_WS_URL=ws://localhost:4000
NEXT_PUBLIC_AGENTS_URL=http://localhost:4001
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_3D=true
NEXT_PUBLIC_ENABLE_VOICE=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# =============================================================================
# PERFORMANCE CONFIGURATION (Ultra-high performance)
# =============================================================================
MAX_CONCURRENT_USERS=1000
MAX_PAGES_PER_TASK=100
CACHE_TTL=3600
CDN_ENABLED=true
EDGE_COMPUTING_ENABLED=true
RESPONSE_TIME_TARGET=100

# =============================================================================
# INTEGRATION CONFIGURATION (Superior to Manus integrations)
# =============================================================================
GOOGLE_SHEETS_API_KEY=your-google-sheets-api-key
NOTION_API_KEY=your-notion-api-key
LINKEDIN_API_KEY=your-linkedin-api-key
ZAPIER_WEBHOOK_URL=your-zapier-webhook-url
WEBHOOK_SECRET=your-webhook-secret

# =============================================================================
# STRIPE CONFIGURATION (Optional)
# =============================================================================
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# =============================================================================
# SEO & ANALYTICS CONFIGURATION
# =============================================================================
GOOGLE_SITE_VERIFICATION=your-google-site-verification-code
GOOGLE_ANALYTICS_ID=your-google-analytics-id
MIXPANEL_TOKEN=your-mixpanel-token

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================
DEBUG_MODE=true
HOT_RELOAD_ENABLED=true
SOURCE_MAPS_ENABLED=true
TYPESCRIPT_STRICT_MODE=true